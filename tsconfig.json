{
	"exclude": ["node_modules"],
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"target": "es2016",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],                                       
		"module": "commonjs",
		"declaration": false,                              
		"outDir": "./dist", 
		"esModuleInterop": true, 
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}